{
  "version": "6.0",
  "tenant": "mytenant",
  "channel": "smtp",
  "runtime": "storm",
  "type": "punchline",
  "name": "input",
  "settings": {
    "additional_jars": [
      "punch-smtp-node-6.1.1-SNAPSHOT.jar"
    ]
  },
  "dag": [
    {
      "type": "third_party_input",
      "class": "com.github.punch.nodes.smtp.SmtpInput",
      "component": "input",
      "settings": {
        "host": "127.0.0.1",
        "port": 2525
      },  
      "publish": [
        {
          "stream": "logs",
          "fields": [
            "log"
          ]
        }
      ]
    },
    {
      "type": "punchlet_node",
      "settings": {
        "punchlet": [
          "punchlets/common/printroot.punch"
        ]
      },
      "component": "punch_bolt",
      "subscribe": [
        {
          "component": "input",
          "stream": "logs"
        }
      ],
      "publish": []
    }
  ]
}
