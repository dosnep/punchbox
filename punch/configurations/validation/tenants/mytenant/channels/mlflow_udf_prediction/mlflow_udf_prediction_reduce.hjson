{
	version: "6.1"
	tenant:  demo
	runtime: pyspark

  dag: [
        {
            type: dataset_generator
            component: input
            settings: {
                input_data: [
                    {
			zero : 0
			first : 1
			second : 2
			third : 3
			fourth : 4
			
                    }
                    {
			zero : 0
			first : 1
			second : 2
			third : 3
			fourth : 4
			
                    }
                    {
			zero : 0
			first : 1
			second : 2
			third : 3
			fourth : 4
			
                    }
                    {
			zero : 0
			first : 1
			second : 2
			third : 3
			fourth : 4
			
                    }
                ]
            }
            publish: [
                {
                    stream: data
                }
            ]
    }
    {
        type: sql
        component: sql
        settings: {
            statement_list: [
                    {
		    	output_table_name: data
                    	statement: SELECT prediction(zero, first, second, third, fourth) as output FROM input_data 
        	    }
	    ]
	}
        subscribe: [
            {
                component: input
                stream: data
            }
        ]
        publish: [
            { 
                stream: data
            }
        ]
    }      
    {
       type: show
       component: print
       subscribe: [
           {
               stream: data
               component: sql
           }
       ]
    }         
  ]
	settings: {
		spark.additional.pex: mlflow_udf.pex
		spark.pre_punchline_execution: nodes.mlflow_udf.udf_loading.mlflow_model_loading_pre_execution.MlflowModelLoadingPreExecution
		spark.punch.mlflow.model.type: clustering 
		spark.punch.mlflow.model.uri: /home/pierre/pp/pp-punch/packagings/punch-standalone/target/punch-standalone-6.2.0-SNAPSHOT-linux/conf/tenants/mytenant/channels/mlflow_rest/KMeans
		spark.executorEnv.ARROW_PRE_0_15_IPC_FORMAT: 1

	}
}
