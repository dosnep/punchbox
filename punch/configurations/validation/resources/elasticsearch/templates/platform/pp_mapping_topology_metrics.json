{
  "order": 8,
  "mappings": {
      "date_detection": true,
      "numeric_detection": false,
      "dynamic_templates": [
        {
          "strings_as_keywords": {
            "match_mapping_type": "string",
            "mapping": {
              "type": "keyword",
              "ignore_above": 256
            }
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date",
          "format": "epoch_millis||strictDateOptionalTime"
        },
        "spark.application.id": {
          "type": "keyword"
        },
        "storm": {
            "type": "object",
            "properties": {
                "worker": {
                    "type": "object",
                    "properties":{
                      "id": {
                        "type": "keyword"
                      }
                    }
                }
            }
        }
      }
  },
  "index_patterns": [
    "*-metrics-*"
  ]
}
